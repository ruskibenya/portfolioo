<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">


<ul class="nav nav-tabs">
  <li class="nav-item">
    <a class="nav-link active" data-toggle="tab" href="#webdev">Web Developer</a>
  </li>
  <li class="nav-item">
    <a class="nav-link" data-toggle="tab" href="#pm">Product Manager</a>
